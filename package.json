{
  "name": "yelp-camp",
  "version": "0.1.0",
  "description": "Simple Yelp Clone",
  "main": "index.js",
  "author": "Cody Brunner <rockchalkwushock@icloud.com> (https://rcws-development.com)",
  "license": "MIT",
  "scripts": {
    "commit": "nps commit",
    "dev": "nps server.dev",
    "precommit": "lint-staged",
    "prettier": "prettier --single-quote --no-semi --write",
    "start": "node --harmony index.js"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn prettier",
      "git add"
    ]
  },
  "dependencies": {
    "body-parser": "~1.17.2",
    "connect-mongo": "~1.3.2",
    "cors": "~2.8.4",
    "dotenv-safe": "~4.0.4",
    "ejs": "~2.5.7",
    "express": "~4.15.4",
    "express-session": "~1.15.5",
    "helmet": "~3.8.1",
    "method-override": "~2.3.9",
    "mongoose": "~4.11.6",
    "morgan": "~1.8.2",
    "passport": "~0.4.0",
    "passport-facebook": "~2.1.1",
    "passport-google-oauth20": "~1.0.0",
    "passport-twitter": "~1.0.4"
  },
  "devDependencies": {
    "commitizen": "~2.9.6",
    "cz-conventional-changelog": "~2.0.0",
    "eslint": "~4.4.1",
    "eslint-config-equimper": "~2.2.1",
    "eslint-config-prettier": "~2.3.0",
    "faker": "~4.1.0",
    "husky": "~0.14.3",
    "lint-staged": "~4.0.3",
    "nodemon": "~1.11.0",
    "nps": "~5.7.0",
    "nps-utils": "~1.3.0",
    "prettier": "~1.5.3"
  }
}
