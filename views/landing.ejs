<% include partials/header %>
  <div class="jumbotron" style="margin-bottom:0; padding:2rem 2rem;">
    <h1 class="display-3">
      <i class="fa fa-compass" aria-hidden="true"></i> YelpCamp
    </h1>
    <p class="lead">A site for those who love camping to come together.</p>
    <hr class="my-4">
    <p>Join our community and start living the good life!</p>
    <p class="lead">
      <a class="btn btn-primary btn-lg" href="/register" role="button">Register</a>
    </p>
  </div>
  <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
    <ol class="carousel-indicators">
      <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
      <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
      <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
      <li data-target="#carouselExampleIndicators" data-slide-to="3"></li>
      <li data-target="#carouselExampleIndicators" data-slide-to="4"></li>
    </ol>
    <div class="carousel-inner">
      <div class="carousel-item active">
        <img class="d-block w-100" src="http://i.imgur.com/K3mPv14.jpg" alt="First slide">
      </div>
      <div class="carousel-item">
        <img class="d-block w-100" src="http://i.imgur.com/SBEmFpv.jpg" alt="Second slide">
      </div>
      <div class="carousel-item">
        <img class="d-block w-100" src="http://i.imgur.com/emvhOnb.jpg" alt="Third slide">
      </div>
      <div class="carousel-item">
        <img class="d-block w-100" src="http://i.imgur.com/2LSMCmJ.jpg" alt="Fourth slide">
      </div>
      <div class="carousel-item">
        <img class="d-block w-100" src="http://i.imgur.com/TVGe0Ef.jpg" alt="Fifth slide">
      </div>
    </div>
  </div>
  <% include partials/footer %>
